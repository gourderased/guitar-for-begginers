{{>layout/header}}


<h1>문의 게시판</h1>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href="/board-save" role="button" class="btn btn-primary">글 등록</a>
        </div>
    </div>

    <br>
    <!-- 목록 출력 영역 -->
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>게시글번호</th>
            <th>제목</th>
            <th>작성자</th>
            <th>작성일</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#boards}}
            <tr>
                <td>{{id}}</td>
                <td>{{title}}</td>
                <td>{{memberLoginId}}</td>
                <td>{{createDate}}</td>
            </tr>
        {{/boards}}
        </tbody>
    </table>
</div>

{{>layout/footer}}

